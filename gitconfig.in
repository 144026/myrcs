; Add your own user info below
; [user]
	; name = user name
	; email = dev email
	; signingkey = gpg signing key/subkey fingerprint
[alias]
	st = status
	a = add
	aa = add -A
	c = commit
	cs = commit  --signoff
	cc = commit  --signoff --gpg-sign --verbose
	d = diff
	dn = diff --name-only
	dw = diff --word-diff
	lg = log --graph --format='%C(red)%h%C(yellow)%d %C(reset)%s %C(green)(%cr) %C(bold blue)<%an>%C(reset)'
	lga = lg --all
	b = branch
	br = branch -vva
	s = switch
	co = checkout
	m = merge
	mnf = merge --no-ff
	pl = pull
	ph = push
	f = fetch
	rp = remote prune
	dh = diff HEAD
[pull]
	rebase = false
[core]
	editor = vim
[push]
	default = upstream
